{
  "name": "node-koa-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "mocha": "mocha test/**/*.test.js --exit",
    "lint": "eslint --ext js ./ src test",
    "pm2": "pm2",
    "start": "npm run pm2 -- start process.yml --no-daemon",
    "test": "npm run lint && npm run mocha"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": [
        "npm run lint && npm test"
      ]
    }
  },
  "keywords": [],
  "author": "Oleksandr",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "8.9.1",
    "axios": "0.17.1",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "eslint": "4.16.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "husky": "0.15.0-rc.3",
    "lint-staged": "6.1.0",
    "mocha": "5.0.0",
    "sinon": "4.2.2"
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "chalk": "2.3.0",
    "http-status-codes": "1.3.0",
    "knex": "0.14.2",
    "koa": "2.4.1",
    "koa-bodyparser": "4.2.0",
    "koa-logger": "3.1.0",
    "koa-passport": "4.0.1",
    "koa-redis": "3.1.2",
    "koa-router": "7.3.0",
    "koa-session": "5.8.1",
    "passport-local": "1.0.0",
    "pg": "7.4.1",
    "pm2": "2.9.3",
    "uuid": "3.2.1",
    "winston": "2.4.0"
  }
}
